{
  "name": "lambda_boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate repo for creating nearly-free APIs using AWS Lambda",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "npm run stop-local && npm run local:setup && jest --testPathPattern=\\.integration\\.test\\.ts --testPathIgnorePatterns=''",
    "test:smoke": "jest --testPathIgnorePatterns='/node_modules/' --testPathPattern='smoke-tests' --testTimeout=60000",
    "test:all": "jest --testPathIgnorePatterns=''",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "start-local": "./scripts/start-local.sh",
    "stop-local": "./scripts/stop-local.sh",
    "deploy-local": "./scripts/deploy-local.sh",
    "local:setup": "npm run start-local && npm run deploy-local"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "dynamodb",
    "api-gateway",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.865.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "aws-sdk-client-mock": "^3.0.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
